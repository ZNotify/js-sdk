{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "outDir": "./dist/",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node"
  }
}
